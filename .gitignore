.eslintcache

# Optional REPL history
.node_repl_history

# Yarn Integrity file
.yarn-integrity

# dotenv environment variables file
.env
.env.test
.env.production
.env.development

# parity-ethereum wasm build artifacts
parity/
parity_wasm/
parity_wasm_packed

# vscode
.vscode/

# Local environment variables
.localenv

